{
  "name": "@utxorpc/lucid-evolution-provider",
  "version": "0.1.6",
  "description": "lucid evolution provider for UTxORPC",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./lib/*": "./lib/*"
  },
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "node --import tsx --test --experimental-fetch src/test/u5c.test.ts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf node_modules && rm -rf lib",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lucid-evolution/core-types": "0.1.22",
    "@lucid-evolution/core-utils": "^0.1.16",
    "@lucid-evolution/lucid": "0.4.29",
    "@lucid-evolution/plutus": "0.1.29",
    "@lucid-evolution/provider": "0.1.90",
    "@lucid-evolution/sign_data": "0.1.25",
    "@lucid-evolution/utils": "0.1.66",
    "@lucid-evolution/wallet": "0.1.72",
    "@utxorpc/sdk": "0.7.0",
    "@utxorpc/spec": "0.17.0"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.42.0"
  }
}
